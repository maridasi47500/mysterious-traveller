<%=form_with model: user, scope: "", url: "/Chat/SettingsDating",html: {class: "form"} do |form|%>
                        <div class="form-group">
                            I'm looking for a relationship
                            <ul class="list-unstyled list-inline dating-list">
<%=form.collection_check_boxes(:relationship_ids, Relationship.all, :id, :name) do |b|%>
<li>
                                    <%=b.label(for: "CB#{b.text}") do%>
                                        <%=b.check_box(name: "DatingSearched#{b.text}", id: "CB#{b.text}", value: "true", class: "custom-checkbox")%>
                                        <%=b.text%>
                                    <%end%>
                                </li>
<%end %>                               

                              
                            </ul>
                        </div>
                        <div class="form-group">
                            With
                            <ul class="list-unstyled list-inline gender-list">
                                <li>
                                    <label for="CBMan" class="text-man">
                                        <%=form.check_box "GenderSearchedMan", id: "CBMan", value: "true", class: "custom-checkbox"%>
                                        a man
                                    </label>
                                </li>
                                <li>
                                    <label for="CBWoman" class="text-woman">
                                        <%=form.check_box "GenderSearchedWoman", id: "CBWoman", value: "true", class: "custom-checkbox"%>
                                        a woman
                                    </label>
                                </li>
                            </ul>
                        </div>
                        <div class="form-group text-center">
                            <input type="submit" class="form-control btn bg-guest hover btn-submit" value="Proceed"/>
                        </div>
                    <%end%>