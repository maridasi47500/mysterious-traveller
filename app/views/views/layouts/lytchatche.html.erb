<body class="no-scroll chat-body">
    
    

    

    

    

    

    

    <img id="flag" src="/Content/Common/Images/ImagesEnabled.png" alt="">

    

    <div class="container chat">
        
        
        
    
<div class="header connected-header row">
    <div class="hidden-xs col-sm-2 col-md-3 col-lg-3 logo-container">
        <span class="icon-logo fs-6x"></span>
        
    </div>
    <div class="col-xs-12 col-sm-10 col-md-9 col-lg-9 header-advert">
        <div class="stretch hidden" data-bind="if: isNotInDynamicContentPage, css: { 'hidden': isInDynamicContentPage }"></div>
        <div class="stretch" data-bind="if: isInDynamicContentPage, css: { 'hidden': isNotInDynamicContentPage }">
       </div>
    </div>
</div>

        
        <div class="chat-content">
            <div class="row stretch-height">
                <div class="hidden-xs col-sm-4 col-md-3 col-lg-3 stretch-height chatters-list-col">
                    <div class="chatters-list-wrapper" data-bind="component: { name: 'chatterslist', params: { event: event, descriptor: descriptor } }">
    <div class="stretch-height">
        <div class="chatters-title title bg-vip">
            <span data-bind="if: nbChattersIsVisible">
                <span class="nb" data-bind="text: numberFirstPart"><%=User.connectedfirstpart%></span>
                <span class="nb" data-bind="text: numberSecondPart"><%=User.connectedsecondpart%></span>
            </span>
            <span data-bind="if: defaultTitleIsVisible">
                <span>Online</span>
            </span>
            <span data-bind="if: researchTitleIsVisible"></span>
        </div>
        <div class="chatters-body">
            <div class="chatters-list-container stretch">
                <ul class="list-unstyled chatters-list custom-scrollbar" data-bind="foreach: chatters, scrollToTop: chatters">
<%=render partial: "interlocutor", collection: User.useronline(current_user.id), as: :user%>             
  </ul>
                <div class="chatters-pagination no-selectable">
                    <ul class="list-unstyled list-inline" data-bind="if: paginationIsVisible">
                        <li>
                            <span class="icon icon-refresh" title="refresh connected list" data-bind="click: refresh"></span>
                        </li>
                        <li>
                            <span class="icon icon-first" title="first page" data-bind="click: goFirstPage"></span>
                        </li>
                        <li>
                            <span class="icon icon-previous" title="previous page" data-bind="click: previous"></span>
                        </li>
                        <li class="text">
                            <span data-bind="text:page">1</span>
                            /
                            <span data-bind="text: nbPages">121</span>
                        </li>
                        <li>
                            <span class="icon icon-next" title="next page" data-bind="click: next"></span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
                    <div class="message-counter-wrapper" data-bind="if: componentXSIsHidden">
                        <div data-bind="component: { name: 'messagecounter', params: { event : event, descriptor: descriptor } }">
    <div class="message-counter-container pointer">
        <a href="/Chat/newinterlocutors" data-bind="click: messageCounterClicked">
            <div class="message-counter bg-vip stretch-height">
                <div class="padding">
                    <span class="icon icon-new-message fs-2x pull-left"></span>
                    <span class="text text-center">
                        <span class="bold hidden-sm hidden-md">new</span>
                        <span>messages</span>
                    </span>
                </div>
                <div class="pull-right message-count bold" data-bind="text: messageCount, blinking: messageCount" style="opacity: 1;">0</div>
            </div>
        </a>
    </div>
</div>
                    </div>
                </div>
               <%=yield%> 
            </div>
        </div>
    </div>
    <div class="stretch-width" data-bind="component: { name: 'messaging', params: { event: event } }">
    <div data-bind="with: message"></div>
</div>
    
