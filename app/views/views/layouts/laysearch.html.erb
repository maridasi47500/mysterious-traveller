<script type="text/html" id="medias-profile-template">
    <div data-bind="if: hasPublicPhotos">
        <div class="form-group">
            <h4 class="text-vip bold">Photos</h4>
            <div class="slider-container" id="profile-slider-pictures" data-bind="customSlider: { 'duration': 250, 'nbSlides': 1 }">
                <div class="slider-wrapper">
                    <ul class="list-unstyled list-inline slider hidden" data-bind="foreach: publicPhotos">
                        <li class="slide profile-media">
                            <div class="stretch item" data-bind="style: { 'background-image': $parent.getBackgroundStyledPictureUrl(url) }, click: $parent.selectPublicPicture"></div>
                        </li>
                    </ul>
                </div>
                <div class="prev slider-btn hidden">
                    <span class="icon-circle-left"></span>
                </div>
                <div class="next slider-btn hidden">
                    <span class="icon-circle-right"></span>
                </div>
            </div>
        </div>
        <div data-bind="with: selectedPublicPicture, click: unselectPublicPicture">
            <div class="zoom-media zoom-picture" data-bind="scrollTo: { container: '.content', element: '#profile-slider-pictures', duration: 200 }">
                <span class="icon icon-close"></span>
                <span class="icon icon-report-abuse" data-bind="click: $parent.reportAbuseOnMedia, clickBubble: false" title="Report an abuse"></span>
                <img data-bind="attr: { 'src': url }" class="img-responsive"/>
            </div>
        </div>
    </div>

    <div data-bind="if: hasPublicVideos">
        <div class="form-group">
            <h4 class="text-vip bold">Videos</h4>
            <div class="slider-container" id="profile-slider-videos" data-bind="customSlider: { 'duration': 250, 'nbSlides': 1 }">
                <div class="slider-wrapper">
                    <ul class="list-unstyled list-inline slider hidden" data-bind="foreach: publicVideos">
                        <li class="slide profile-media">
                            <div class="stretch item" data-bind="style: { 'background-image': $parent.getBackgroundStyledPictureUrl(urlPreview) }"></div>
                            <div class="stretch" data-bind="click: $parent.selectPublicVideo">
                                <div class="overlay stretch"></div>
                                <span class="icon icon-next"></span>
                            </div>
                        </li>
                    </ul>
                </div>
                <div class="prev slider-btn hidden">
                    <span class="icon-circle-left"></span>
                </div>
                <div class="next slider-btn hidden">
                    <span class="icon-circle-right"></span>
                </div>
            </div>
        </div>
        <div data-bind="with: selectedPublicVideo">
            <div class="zoom-media zoom-video" data-bind="scrollTo: { container: '.content', element: '#profile-slider-videos', duration: 200 }">
                <span class="icon icon-close pointer" data-bind="click: $parent.unselectPublicVideo"></span>
                <span class="icon icon-report-abuse pointer" data-bind="click: $parent.reportAbuseOnMedia, clickBubble: false" title="Report an abuse"></span>
                <video class="pointer" controls="controls" type="video/mp4" data-bind="attr: { 'src': urlMP4 }"></video>
            </div>
        </div>
    </div>
</script>

