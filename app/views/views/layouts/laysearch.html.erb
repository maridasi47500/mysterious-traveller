<script type="text/html" id="default-chatter-info-template">
    <div class="chatter-infos-container fix-height" data-bind="css:{ 'man': chatter.isAMan, 'woman': chatter.isAWoman }">
        <div class="pull-left chatter-picture" data-bind="if: chatter.hasProfilePicture">
            <div class="chatter-picture-template" data-bind="template : { name : pictureTemplate }"></div>
        </div>
        <div class="pull-left chatter-picture" data-bind="if: !chatter.hasProfilePicture">
            <div class="avatar-container" data-bind="css: { 'avatar-man': chatter.isAMan, 'avatar-woman': chatter.isAWoman }">
                
                <div class="online">
                    <span class="icon icon-online" data-bind="css: { 'online': chatter.isConnected, 'offline': chatter.isDisconnected }"></span>
                </div>
            </div>
        </div>
        <div class="chatter-infos">
            <span class="chatter-name" data-bind="text: chatter.name"></span>
            <div class="infos">
                <div class="row-infos age">
                    <span data-bind="if: chatter.isAMan">
                        Man,
                    </span>
                    <span data-bind="if: chatter.isAWoman">
                        Woman,
                    </span>
                    <span data-bind="text: chatter.age"></span> years old
                </div>
                <div class="row-infos">
                    <span data-bind="text: chatter.country"></span>
                    <span data-bind="text: chatter.htmlSubdivision"></span>
                </div>
                <div class="infos-row">
                    <span data-bind="if: chatter.vip">
                        <span class="icon icon-vip"></span>
                    </span>
                    <span data-bind="if: chatter.isLookingForMan">
                        <span class="icon icon-man text-man" title="Looking for a man"></span>
                    </span>
                    <span data-bind="if: chatter.isLookingForWoman">
                        <span class="icon icon-woman text-woman" title="Looking for a woman"></span>
                    </span>
                    <span data-bind="if: chatter.isLookingForBoth">
                        <span class="icon icon-couple text-man-woman" title="Looking for a man or woman"></span>
                    </span>
                    <span data-bind="if: chatter.vip && chatter.isLookingForUnknown">
                        <span class="icon icon-spacer"></span>
                    </span>
                </div>
            </div>
        </div>
    </div>
</script>

