nt(htmlMarginTop));\n    var siteWidth = zoneScale * 1000;\n    var sideWidth = Math.ceil((top.document.documentElement.clientWidth - siteWidth) / 2);\n    var sideHeight = Math.ceil(1000 - fromTop);\n    var gutterWidth = Math.ceil(140 * zoneScale);\n\n    /*==========  CHANGE BODY BACKGROUND  ==========*/\n    body.style.backgroundSize = 1800 * zoneScale + \"px\";\n    // ayads.addcss(\'body{background-size: \'+1800 * zoneScale+\'px !important;}\');\n    ayads.addcss(\"body{background-image:url(\'\" + getSkinzArrInfo().bg + \"\') !important}\");\n\n    /*==========  PRIVACY ICON  ==========*/\n    var privacyLink = top.document.createElement(\'a\');\n    privacyLink.href = data.privacy.optout_click_url;\n    privacyLink.classList.add(\'ayads-privacy-image\');\n    privacyLink.classList.add(\'privacy-\'+getSkinzArrInfo().domain);\n    privacyLink.style.marginTop = mrTop + \"px\";\n    privacyLink.target = \"_blank\";\n    privacyLink.innerHTML = \"<img src=\'\" + data.privacy.optout_image_url + \"\'>\";\n    privacyLink.style.right = sideWidth + \"px\";\n    body.insertBefore(privacyLink, body.firstChild);\n\n    /*==========  CREATE LEFT SIDE  ==========*/\n    var ayadsShopLeft = top.document.createElement(\'div\');\n    ayadsShopLeft.id = \"ayads-shop-left\";\n    ayadsShopLeft.className = \"wingz-\" + getSkinzArrInfo().domain;\n    ayadsShopLeft.style.width = gutterWidth + \"px\";\n    // ayadsShopLeft.style.top = fromTop + \"px\";\n    ayadsShopLeft.style.left = sideWidth + \"px\";\n    body.insertBefore(ayadsShopLeft, body.firstChild);\n\n    /*==========  CREATE RIGHT SIDE  ==========*/\n    var ayadsShopRight = top.document.createElement(\'div\');\n    ayadsShopRight.id = \"ayads-shop-right\";\n    ayadsShopRight.className = \"wingz-\" + getSkinzArrInfo().domain;\n    ayadsShopRight.style.width = gutterWidth + \"px\";\n    // ayadsShopRight.style.top = fromTop + \"px\";\n    ayadsShopRight.style.right = sideWidth + \"px\";\n    body.insertBefore(ayadsShopRi